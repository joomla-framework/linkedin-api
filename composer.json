{
    "name": "joomla/linkedin",
    "type": "joomla-package",
    "description": "Joomla Linkedin Package",
    "keywords": ["joomla", "framework", "linkedin"],
    "homepage": "https://github.com/joomla-framework/linkedin-api",
    "license": "GPL-2.0+",
    "require": {
        "php": "~7.0",
        "joomla/oauth1": "~2.0@dev",
        "joomla/input": "~1.0|~2.0",
        "joomla/http": "^1.2.2|~2.0"
    },
    "require-dev": {
        "joomla/registry": "^1.4.5|~2.0",
        "phpunit/phpunit": "~6.0",
        "squizlabs/php_codesniffer": "1.*"
    },
    "autoload": {
    	"psr-4": {
    		"Joomla\\Linkedin\\": "src/"
    	}
    },
    "autoload-dev": {
        "psr-4": {
            "Joomla\\Linkedin\\Tests\\": "Tests/"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
	        "dev-2.0-dev": "2.0-dev"
        }
    }
}

